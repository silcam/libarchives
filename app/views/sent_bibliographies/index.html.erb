<p id="notice"><%= notice %></p>

<h1>Sent Bibliographies</h1>

<table>
  <thead>
    <tr>
      <th>Archive</th>
      <th>Archive number</th>
      <th>Destination</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sent_bibliographies.each do |sent_bibliography| %>
      <tr>
        <td><%= sent_bibliography.archive %></td>
        <td><%= sent_bibliography.archive_number %></td>
        <td><%= sent_bibliography.destination %></td>
        <td><%= sent_bibliography.date %></td>
        <td><%= link_to 'Show', sent_bibliography %></td>
        <td><%= link_to 'Edit', edit_sent_bibliography_path(sent_bibliography) %></td>
        <td><%= link_to 'Destroy', sent_bibliography, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sent Bibliography', new_sent_bibliography_path %>
