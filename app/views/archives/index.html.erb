<p id="notice"><%= notice %></p>

<h1>Archives</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Document type</th>
      <th>Backup number</th>
      <th>Capacity</th>
      <th>Year</th>
      <th>Title</th>
      <th>English title</th>
      <th>French title</th>
      <th>Title of book reviewed</th>
      <th>Author book reviewed</th>
      <th>Journal name</th>
      <th>Journal vol</th>
      <th>Journal num</th>
      <th>Title of book containing article</th>
      <th>Details of referenced book</th>
      <th>Series editor</th>
      <th>Series name</th>
      <th>Series number</th>
      <th>Place of publication</th>
      <th>Publisher</th>
      <th>Thesis type</th>
      <th>University</th>
      <th>Conference info</th>
      <th>Pages</th>
      <th>Edition</th>
      <th>Copies printed</th>
      <th>Illustrations</th>
      <th>Summary</th>
      <th>Language</th>
      <th>Bilingual notes</th>
      <th>Microfiche info</th>
      <th>Notes</th>
      <th>Web address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @archives.each do |archive| %>
      <tr>
        <td><%= archive.number %></td>
        <td><%= archive.document_type %></td>
        <td><%= archive.backup_number %></td>
        <td><%= archive.capacity %></td>
        <td><%= archive.year %></td>
        <td><%= archive.title %></td>
        <td><%= archive.english_title %></td>
        <td><%= archive.french_title %></td>
        <td><%= archive.title_of_book_reviewed %></td>
        <td><%= archive.author_book_reviewed %></td>
        <td><%= archive.journal_name %></td>
        <td><%= archive.journal_vol %></td>
        <td><%= archive.journal_num %></td>
        <td><%= archive.title_of_book_containing_article %></td>
        <td><%= archive.details_of_referenced_book %></td>
        <td><%= archive.series_editor %></td>
        <td><%= archive.series_name %></td>
        <td><%= archive.series_number %></td>
        <td><%= archive.place_of_publication %></td>
        <td><%= archive.publisher %></td>
        <td><%= archive.thesis_type %></td>
        <td><%= archive.university %></td>
        <td><%= archive.conference_info %></td>
        <td><%= archive.pages %></td>
        <td><%= archive.edition %></td>
        <td><%= archive.copies_printed %></td>
        <td><%= archive.illustrations %></td>
        <td><%= archive.summary %></td>
        <td><%= archive.language %></td>
        <td><%= archive.bilingual_notes %></td>
        <td><%= archive.microfiche_info %></td>
        <td><%= archive.notes %></td>
        <td><%= archive.web_address %></td>
        <td><%= link_to 'Show', archive %></td>
        <td><%= link_to 'Edit', edit_archive_path(archive) %></td>
        <td><%= link_to 'Destroy', archive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Archive', new_archive_path %>
